{
  "name": "vue-space",
  "version": "0.1.4-0",
  "description": "",
  "main": "dist/vue-space.common.js",
  "web": "dist/vue-space.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "postinstall": "shx rm -rf node_modules/@types/react && echo 'workaround for libs importing @types/react/global.d.ts on vue project'",
    "build": "tsc --build && rollup -c ./rollup.config.js",
    "pb": "npm run build && np --no-tests",
    "storybook": "start-storybook -p 6006 --debug-webpack",
    "build-storybook": "build-storybook",
    "lint:fix": "eslint --fix ./{src,stories}/**/*.{tsx,ts,js}",
    "commit": "git-cz",
    "commit:doc": "git-cz --no-verify",
    "lint-staged": "lint-staged -d"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:vue-space/vue-space.git"
  },
  "homepage": "https://github.com/vue-space/vue-space",
  "keywords": [
    "vue",
    "components"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vue-space/vue-space/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-numeric-separator": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-typescript": "^7.7.4",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-backgrounds": "^5.2.8",
    "@storybook/addon-centered": "^5.2.8",
    "@storybook/addon-docs": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addon-storysource": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/vue": "^5.2.8",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-preset-jsx": "^1.1.2",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "babel-preset-vca-jsx": "^0.3.2",
    "babel-preset-vue": "^2.0.2",
    "commitizen": "^4.0.3",
    "cssnano": "^4.1.10",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "lodash": "^4.17.15",
    "np": "^5.2.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-scss": "^2.0.0",
    "prettier": "^1.19.1",
    "rollup": "^1.27.10",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-size-snapshot": "^0.10.0",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "shx": "^0.3.2",
    "stylelint": "^12.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.2.0",
    "stylelint-order": "^3.1.1",
    "stylelint-scss": "^3.13.0",
    "typescript": "^3.7.3",
    "vue-loader": "^15.7.2",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "@vue/composition-api": "^0.3.4",
    "vue": "^2.6.10",
    "vue-functional-data-merge": "^3.1.0"
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{stories,src}/**/*.scss": "stylelint --syntax=scss",
    "{stories,src}/**/*.{ts,tsx,js}": [
      "eslint --fix",
      "git add"
    ]
  }
}
